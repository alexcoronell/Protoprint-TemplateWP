main
    section#contenido-quienessomos.contenido-paginasinternas.pb-4
        .container
            .row
                article.col-12.col-sm-6.col-md-4.imagen-quienessomos.my-md-3.wow.fadeIn(data-wow-duration="2s" data-wow-delay="1.4s" data-wow-offset="60")
                    img(src="img/logo.svg" alt="Logo de Protoprint" title="Protoprint")
                article.col-12.col-sm-6.col-md-8.mt-md-4.wow.fadeIn(data-wow-duration="5s" data-wow-delay="1.6s" data-wow-offset="60")
                    h3.tituloprincipal-quienessomos= `${nameSection}`
                    p= `${content}`
                each seccionEmpresa in seccionesempresa
                    article.col-12.col-sm-6.col-md-4.contenidosMisionVisionValores
                        <button id=#{seccionEmpresa.classButton} class="titulos-quienessomos text-center wow fadeIn" data-wow-duration="2s" data-wow-delay="1.8s" data-wow-offset="60">
                            h4= `${seccionEmpresa.titulo}`
                        </button>
                        <div class=#{seccionEmpresa.classContenido}>
                            - var n = seccionEmpresa.contenidos.length
                            while n > 0
                                p= `${seccionEmpresa.contenidos}`
                                - n = n - 1
                        </div>
