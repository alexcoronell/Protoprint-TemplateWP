section#principal
    #carouselPrincipal.carousel.slide(data-ride="carousel")
        ol.carousel-indicators
            li.active(data-target="#carouselPrincipal" data-slide-to="0")
            li(data-target="#carouselPrincipal" data-slide-to="1")
            li(data-target="#carouselPrincipal" data-slide-to="2")
        .carousel-inner
            .carousel-item.active
                img.d-block.img-fluid(src="img/01-impresiondigital.jpg")
            .carousel-item
                img.d-block.img-fluid(src="img/02-impresionkonica.jpg")
            .carousel-item
                img.d-block.img-fluid(src="img/03-domicilios.jpg")
        a.carousel-control-prev(href="#carouselPrincipal" role="button" data-slide="prev")
            span.carousel-control-prev-icon(aria-hidden="true")
            span.sr-only Anterior
        a.carousel-control-next(href="#carouselPrincipal" role="button" data-slide="next")
            span.carousel-control-next-icon(aria-hidden="true")
            span.sr-only Siguiente