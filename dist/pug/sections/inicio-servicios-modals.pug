include ../data/servicios-data

each servicio in servicios
    - var nombremodales = servicio.nombreModal 
    <div id=#{nombremodales} class="modal fade" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        .modal-dialog(role="document")
            .modal-content
                .modal-header
                    h5.modal-title#exampleModalLabel Impresi√≥n DIgital
                    button.close(type="button" data-dismiss="modal" aria-label="Close")
                        span(aria-hidden="true") &times;
                .modal-body.container
                    .row
                        .col-12.col-md-4
                            img.img-fluid(src= servicio.urlImg alt= servicio.titulo title= servicio.titulo)
                        .col-12.col-md-8
                            p= `${servicio.content}`
                .modal-footer
                    button.btn.btn-secondary(type="button" data-dismiss="modal") Close
    </div>