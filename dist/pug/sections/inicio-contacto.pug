include inicio-contacto-data

section#contacto
    .container
        .row
            .titulosections.col-12.col-md-3
                h2= `${titulosection}`
            .col-12.col-md-9.container
                .row
                    .col-12.cajaformulario.container
                        form.row.form-horizontal#contactoForm(action="ajaxSubmit.php" method="POST")
                            .col-12.text-center.wow.flash(data-wow-duration=".5s" data-wow-delay="0.2s" data-wow-offset="60")
                                h4= `${mensajeFormulario}`
                            .col-12.col-md-4.form-group.wow.fadeIn(data-wow-duration="1s" data-wow-delay="0.2s" data-wow-offset="60")
                                input.form-control#nombre(type="text" name="nombre" placeholder="Tu nombre:" required data-toggle="tooltip" data-placement="top" title="Nombre")
                            .col-12.col-md-4.form-group.wow.fadeIn(data-wow-duration="1s" data-wow-delay="0.4s" data-wow-offset="60")
                                input.form-control#telefono(type="text" name="telefono" placeholder="Tu teléfono:" required data-toggle="tooltip" data-placement="top" title="Teléfono")
                            .col-12.col-md-4.form-group.wow.fadeIn(data-wow-duration="1s" data-wow-delay="0.6s" data-wow-offset="60")
                                input.form-control#email(type="email" name="email" placeholder="Tu email:" required data-toggle="tooltip" data-placement="top" title="Email")
                            .form-group.col-12.wow.fadeIn(data-wow-duration="1s" data-wow-delay="0.8s" data-wow-offset="60")
                                textarea.form-control#mensaje(name="mensaje" placeholder="Tu mensaje" required data-toggle="tooltip" data-placement="top" title="Mensaje")
                            .form-group.col-12.wow.fadeIn(data-wow-duration="1s" data-wow-delay="1s" data-wow-offset="60")
                                button.btn.btn-primary.boton-formulario(type="submit") Enviar
                            .offset-2.col-8.mb-2#correcto= `${mensajeEnviado}`

                            // Empiezan los papas

                    .col-12.caja-mapas.container
                        .row
                            .col-12.col-md-6.anima1
                                .mapainteractivo.text-center.wow.zoomIn(data-wow-duration="1s" data-wow-delay="1.2s" data-wow-offset="60")
                                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3335.0511718332837!2d-75.56932643457532!3d6.254958767773654!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8e4428fc4179b3b1%3A0x4b16809c9761eb24!2sCra.%2054%20%2354-72%2C%20Medell%C3%ADn%2C%20Antioquia!5e0!3m2!1ses-419!2sco!4v1580213923737!5m2!1ses-419!2sco" allowfullscreen=""></iframe>
                                    h5 Sede Centro

                            .col-12.col-md-6.anima2
                                .mapainteractivo.text-center.wow.zoomIn(data-wow-duration="1s" data-wow-delay="1.4s" data-wow-offset="60")
                                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3966.2888325557083!2d-75.60645648590676!3d6.225596528283532!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8e4429ec65f66eed%3A0x3bd8cd0103094042!2sCra.%2082b%20%2320-32%2C%20Medell%C3%ADn%2C%20Antioquia!5e0!3m2!1ses-419!2sco!4v1580215123134!5m2!1ses-419!2sco" allowfullscreen=""></iframe>
